package day1;

public class LoopTest {

	public static void main(String[] args) {
		// for
		for (int i = 0; i < 10; i++) {
			System.out.println("****");
		}
		
		int dan = 3;
		for (int i = 1; i <= 9; i++) {
			System.out.printf("%d * %d = %d\n", dan, i, dan * i);
		}
		System.out.println();
		for (int i = 2; i <= 9; i++) {
			for (int j = 1; j <= 9; j++) {
				System.out.printf("%d * %d = %d\n", i, j, i*j);
			}
		}
	}

}
