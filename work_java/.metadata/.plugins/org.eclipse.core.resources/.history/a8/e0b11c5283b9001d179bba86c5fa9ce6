package com.kb.sms;

import java.util.Random;
import java.util.Scanner;

public class Test5 {

	public static void main(String[] args) {
		// 사용1
		Random r = new Random();
		int num = r.nextInt();
		System.out.println(num);
		
		// 사용2 - static 메서드 
		int a = Math.abs(-34);
		System.out.println(a);
	}


}
