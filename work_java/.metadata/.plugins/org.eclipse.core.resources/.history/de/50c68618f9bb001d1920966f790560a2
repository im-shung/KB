import java.util.*;

class Node {
	int y;
	int x;
	int length;

	public Node(int y, int x, int length) {
		this.y = y;
		this.x = x;
		this.length = length;
	}
}

class Solution {
	static final int[] DY = {1, -1, 0, 0};
	static final int[] DX = {0, 0, 1, -1};
	
	static final int N = 50;
	static int[][] map;
	static int answer = Integer.MAX_VALUE;

	public int solution(int[][] rectangle, int characterX, int characterY, int itemX, int itemY) {
		map = new int[N][N];
		int len = rectangle.length;

		// 직사각형 경계 분리
		// 경계는 1
		for (int i = 0; i < len; i++) {
			// rectangle[i]: [x1, y1, x2, y2]
			int x1 = rectangle[i][0];
			int y1 = rectangle[i][1];
			int x2 = rectangle[i][2];
			int y2 = rectangle[i][3];
			for (int x = x1; x <= x2; x++) {
                map[y1][x] = 1;
                map[y2][x] = 2;
			}
		}
        
        // 직사각형 내부는 0
        for (int i = 0; i < len; i++) {
			// rectangle[i]: [x1, y1, x2, y2]
			int x1 = rectangle[i][0];
			int y1 = rectangle[i][1];
			int x2 = rectangle[i][2];
			int y2 = rectangle[i][3];
			for (int x = x1 + 1; x < x2; x++) {
                for (int y = y1 + 1; y < y2; y++) {
                    map[y][x] = 0;
                }
			}
		}
		System.out.println(Arrays.deepToString(map));
		
		return answer;
	}

	public void bfs(int characterX, int characterY, int itemX, int itemY) {
		Deque<Node> que = new ArrayDeque<>();
		que.add(new Node(characterY, characterX, 0));

		while (!que.isEmpty()) {
			// 1. 큐에서 꺼냄
			Node cur = que.poll();
			// 2. 목적지인가? -> 아이템 위치 
			if (cur.y == itemY && cur.x == itemX) {
				answer = Math.min(answer, cur.length);
			} else {
				// 3. 연결된 곳인가?
				for (int i = 0; i < 4; i++) {
					int dy = cur.y + DY[i];
					int dx = cur.x + DX[i];
					
					// 4. 갈 수 있는가? -> 직사각형의 경계만 갈 수 있음
					if (0 <= dy && dy < N && 0 <= dx && dx < N) {
						if (map[dy][dx] == 1) {
							// 5. 체크인
							map[dy][dx] = 0;
							// 6. 큐에 넣음
							que.add(new Node(dy, dx, cur.length + 1));
						}
					}
				}
			}
		}
	}
	

}