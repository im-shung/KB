package com.itskb.ws01;

import java.util.Random;

public class RandomUtil {
	static Random random = new Random();

	public static int getRandomInt1(int from, int to) {
		// 0 ~ to - 0 ~ from
		System.out.println((int) Math.round(Math.random() * to));
		return (int) Math.round(Math.random() * to) - (int) Math.round(Math.random() * from) + 1;
	}

	public static int getRandomInt2(int from, int to) {
		// 0~to - 0~from + 1
		return random.nextInt(to) - random.nextInt(from) + 1;
	}
}
